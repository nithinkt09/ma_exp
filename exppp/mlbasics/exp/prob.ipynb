    "import math\n",
    "\n",
    "# probability that n people in a room with at least 2 sharing a birthday\n",
    "\n",
    "def prob_birthday_share(n):\n",
    "    return 1 - math.factorial(365) / (math.factorial(365-n) * (365 ** n))\n",
    "\n",
    "\n",
    "print(prob_birthday_share(20))"
   ]
  }
